  import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc = new Scanner(System.in);
      String str = sc.nextLine();
      int count=0;
      for(int i=0;i<str.length();i++){
        char c=str.charAt(i);
        if(Character.isLowerCase(c)){
          count++;
        }
      }
     
      System.out.println(str.length()==count);
    }
}
OUTPUT:
Input:
OC
Output:
false
Input:
oc
Output:
true





 


 
 
 

  
