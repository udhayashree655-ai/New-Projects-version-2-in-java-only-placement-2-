  2)
  import java.util.Scanner;

public class BorderSum {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int r = sc.nextInt();  
        int c = sc.nextInt();  

        int[][] arr = new int[r][c];

       
        for(int i = 0; i < r; i++) {
            for(int j = 0; j < c; j++) {
                arr[i][j] = sc.nextInt();
            }
        }

        int sum = 0;

       
        for(int i = 0; i < r; i++) {
            for(int j = 0; j < c; j++) {
                if(i == 0 || i == r-1 || j == 0 || j == c-1) {
                    sum += arr[i][j];
                }
            }
        }

        System.out.println(sum);
    }
}
output:
3
3

1 2 3 4 5 6 7 8 9
40
  3)
  import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int size = sc.nextInt();  
       

        int[][] arr = new int[size][size];

       
        for(int i = 0; i < size; i++) {
            for(int j = 0; j < size; j++) {
                arr[i][j] = sc.nextInt();
            }
        }

        int sum = 0;

       
        for(int i = 0; i < size; i++) {
            for(int j = 0; j < size; j++) {
                if(i == j || i+j==size-1) {
                    sum += arr[i][j];
                }
            }
        }

        System.out.println(sum);
    }
}
output:
3
1 2 3 4 5 6 7 8 9
25
