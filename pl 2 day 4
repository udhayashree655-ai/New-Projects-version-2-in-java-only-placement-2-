  import java.util.*;
public class Solution {
  public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        int arr[] = new int[num];
        for(int i=0;i<num;i++){
            arr[i] = sc.nextInt();
        }
        for(int i=0;i<num;i++){
            System.out.print(arr[i]+" ");
        }
   }
}
output:
4
-1 -2 -3 -4
  2)
  import java.util.Scanner;

public class ArraySum {
    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);

        
        int N = sc.nextInt();

        int[] arr = new int[N];
        int sum = 0;


        for (int i = 0; i < N; i++) {
            arr[i] = sc.nextInt();
            sum += arr[i];
        }

       
        System.out.println("Sum of all elements: " + sum);

        sc.close();
    }
}
output:
5
1 2 3 4 5
Sample Output 0

15
