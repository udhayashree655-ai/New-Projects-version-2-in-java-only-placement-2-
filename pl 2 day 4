  import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int r =sc. nextInt();
        int arr[] =new int[r];
        for(int i=0;i<r;i++){
            arr[i] = sc.nextInt();
        }
        int search = sc.nextInt();
        //int index=-1;
        int f=0;
        for(int i=0;i<r;i++){
            if(search==arr[i]){
                f=1;//index=1;
                System.out.println(i);
                break;
            }
        }
        //system.out.println(index);
        if(f==0){
            System.out.println("-1");
               }
       }
}
  output:
  input (stdin)

5
1 2 3 4 5
3
Your Output (stdout)

2
Expected Output

2
8)
    
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int t = sc.nextInt();
        int arr[]= new int[t];
        for(int i=0;i<t;i++){
            arr[i]= sc.nextInt();
        }
        int max = Integer.MIN_VALUE;
        for(int i=0;i<t;i++){
            if(arr[i]>max){
            max = arr[i];
            }
    }
        System.out.println(max);
}
}
  output:
  5
2 8 1 6 3
Sample Output 0

8
Sample Input 1

5
-3 -7 -1 -9 -4
Sample Output 1

-1
