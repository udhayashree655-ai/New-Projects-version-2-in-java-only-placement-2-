  import java.util.*;
public class Solution {
  public static void main(String[] args) {
      Scanner sc = new Scanner(System.in);
      int q = sc.nextInt();
      int arr[] = new int[q];
      for(int i=0;i<q;i++){
          arr[i] = sc.nextInt();
          
      }
      for(int i=0;i<q;i++){
      System.out.print(arr[i]+" ");    
      }
      System.out.println();
      for(int i=q-1;i>=0;i--){
          System.out.print(arr[i]+" ");
      }
        
    }
