INORDER,PREORDER,POSTORDER TREE DATA STRUCTURE IN TREE:
class Node {
    int data;
    Node left;
    Node right;

    Node(int d) {   
        data = d;
        left = null;
        right = null;
    }
}
public class Main {
    public static void main(String[] args) {
        Node root = new Node(10);
        Node n1 = new Node(20);   
        root.left = n1;
        Node n2 = new Node(30);   
        root.right = n2;
        Node n3 = new Node(40);   
        root.left.right = n3;
        System.out.println("root data " + root.data);
    }
}
OUTPUT:
root data 10

TREE INORDER:
class Node {
    int data;
    Node left;
    Node right;

    Node(int d) {   
        data = d;
        left = null;
        right = null;
    }
}

class Binarytree {
    Node root = null;
    
    void add(int data) {
        if(root == null) {
            root = new Node(data);
        }
        else {
            childadd(root,data);
        }
    }
    void childhood(Node root,int data) {
        if(root.left== null);
        root.left = new Node(data);
    }
    else if(root.right == null) {
        childadd(root.left,data);   
    }
}
    void inorder(Node root) {
        if(root == null) {
            return;
        }
        inorder(root,left);
        Sytem.out.println(root.dat+" ");
        inorder(root.right);
    }
    
    void preorder(Node root) {
        iof(root == null) {
            return;
        }
        System.out.println(root.dat+" ");
        preorder(root.left);
        preorder(root.right);
    }
    void postorder(Node root) {
        if(root ==  null) {
            return;
        }
    postorder(root.left);
    postorder(root.right);
    System.out.print(root.data+" ");
    }
}
    public class Main {
    public static void main(String[] args) {
Binarytree b = new Binarytree();
      b.tree(10);
      b.tree(20);
      b.tree(30);
      b.tree(40);
      b.tree(50);
      b.tree(60);
      
        System.out.println("root data " + root.data);
    }
}
